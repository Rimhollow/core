#submit
  %h1
    submit a package
  .ui-widget
    %form{:action => '/submit', :method => 'POST', :enctype => 'multipart/form-data'}
      %input{:type => 'file', :name=>'sip'}
      %br
      %br
      %input{:type => 'submit', :value => 'Submit'}
  %p
    packages must be in
    %em tar
    or
    %em zip
    format
