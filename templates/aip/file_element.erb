<file ID='file-<%= df.id %>'
      OWNERID='<%= df.uri %>'
      ADMID='<%= @admid_map[df.id].join ' ' %>'
      SIZE='<%= df.size %>'
      CHECKSUMTYPE='SHA-1'
      CHECKSUM='<%= df.open { |io| Digest::SHA1.hexdigest io.read } %>'>
  <% unless df.obsolete? %>
    <FLocat LOCTYPE='URL' xlink:href='<%= df['aip-path'] %>'/>
  <% end %>
</file>
